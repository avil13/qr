{
  "name": "qr-reader",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "DEBUG='vite-ssg:*' vite",
    "build": "run-p type-check \"build-only {@}\" --",
